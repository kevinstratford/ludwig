##############################################################################
#
#  serial-tern-st1.inp
#
#  Initialisation from "2d block double emulsion" in a 120x120 system.
#  The parameters are similar to those used in Semprebon et al (2016)
#  Figure 1e.
#
#  This particular case is relaxational dynamics only
#  ("hydrodynamics off").
#
#  The test is run only for 10 steps.
#
##############################################################################

##############################################################################
#
#  Run duration
#
###############################################################################

N_cycles 10

##############################################################################
#
#  System
#
##############################################################################

size 180_120_1
grid 1_1_1

##############################################################################
#
#  Fluid parameters
#
##############################################################################

viscosity 0.16666666666667

##############################################################################
#
#  Free energy parameters
#
###############################################################################

free_energy ternary

ternary_kappa1            0.01
ternary_kappa2            0.02
ternary_kappa3            0.05
ternary_alpha             1.00

ternary_mobility_phi      0.15
ternary_mobility_psi      0.10

ternary_initialisation    2d_double_emulsion

hydrodynamics             no
fd_gradient_calculation   2d_tomita_fluid

###############################################################################
#
#  Colloid parameters
#
###############################################################################

colloid_init        no_colloids

###############################################################################
#
#  Periodic conditions / boundaries
#
###############################################################################

periodicity 1_1_1

###############################################################################
#
#  Output frequency and type
#
###############################################################################

freq_statistics 10
config_at_end   no
